{
  "name": "yarr-stremio-addon",
  "version": "0.1.0",
  "description": "YARR! - A high-performance Stremio addon featuring 60+ torrent sources, smart ranking, multi-debrid support, and advanced filtering capabilities",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node -e \"require('fs-extra').copySync('src/addon/landing.html', 'dist/addon/landing.html') && require('fs-extra').copySync('src/iptv/m3u-sources', 'dist/iptv/m3u-sources')\"",
    "start": "node --no-warnings dist/index.js",
    "dev": "esrun --node-no-warnings --watch src/index.ts"
  },
  "keywords": [
    "stremio",
    "addon",
    "torrent",
    "streaming",
    "yarr",
    "aggregator",
    "debrid",
    "1337x",
    "thepiratebay",
    "rarbg"
  ],
  "author": "YARR! Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spookyhost1/yarr-stremio.git"
  },
  "bugs": {
    "url": "https://github.com/spookyhost1/yarr-stremio/issues"
  },
  "homepage": "https://github.com/spookyhost1/yarr-stremio#readme",
  "devDependencies": {
    "@types/bytes": "^3.1.4",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/localtunnel": "^2.0.4",
    "@types/node": "^20.11.20",
    "@types/stremio-addon-sdk": "^1.6.11",
    "@types/tough-cookie": "^4.0.5",
    "@types/webtorrent": "~0.109.8",
    "esrun": "^3.2.26",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "all-debrid-api": "^1.2.0",
    "axios": "1.12.2",
    "axios-cookiejar-support": "^4.0.7",
    "cheerio": "1.0.0-rc.12",
    "debrid-link-api": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "eztv-crawler": "^1.3.6",
    "fs-extra": "^11.2.0",
    "localtunnel": "^2.0.2",
    "magnet-uri": "^7.0.7",
    "memory-chunk-store": "^1.3.5",
    "mime": "^4.0.1",
    "named-queue": "^2.2.1",
    "p-limit": "^6.1.0",
    "parse-torrent-title": "^2.1.0",
    "premiumize-api": "^1.0.3",
    "real-debrid-api": "git://github.com/TheBeastLT/node-real-debrid.git#d1f7eaa8593b947edbfbc8a92a176448b48ef445",
    "rss-to-json": "^2.1.1",
    "stremio-addon-sdk": "^1.6.10",
    "tough-cookie": "^4.1.3",
    "ts-jackett-api": "^1.0.0",
    "webtorrent": "^2.4.2",
    "yts-api-node": "^1.1.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "stremio-addon-sdk@1.6.10": "patches/stremio-addon-sdk@1.6.10.patch"
    },
    "onlyBuiltDependencies": [
      "bufferutil",
      "esbuild",
      "eztv-crawler",
      "fs-native-extensions",
      "node-datachannel",
      "utf-8-validate",
      "utp-native",
      "yts-api-node"
    ]
  }
}
